{
  "name": "@runroom/stylelint-config-runroom",
  "version": "1.3.0",
  "description": "Runroom config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "runroom"
  ],
  "author": {
    "name": "Runroom",
    "url": "https://runroom.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Runroom/stylelint-config-runroom.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "stylelint": "11.1.1",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "3.1.1",
    "stylelint-scss": "3.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/register": "7.6.2",
    "@runroom/eslint-config-runroom": "1.0.1",
    "ava": "2.4.0",
    "babel-preset-env": "1.7.0",
    "npm-run-all": "4.1.5",
    "npmpub": "5.0.0",
    "remark-cli": "7.0.0",
    "remark-preset-lint-consistent": "2.0.3",
    "remark-preset-lint-recommended": "3.0.3"
  },
  "scripts": {
    "ava": "ava --verbose \"__tests__/index.js\"",
    "lint": "eslint . --config=.eslintrc",
    "test": "yarn run ava"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  }
}
