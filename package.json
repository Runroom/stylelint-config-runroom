{
  "name": "stylelint-config-runroom",
  "version": "1.1.3",
  "description": "Runroom config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "runroom"
  ],
  "author": "Runroom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/italodr/stylelint-config-runroom.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "ava": "0.22.0",
    "babel-preset-env": "1.6.0",
    "eslint": "4.8.0",
    "eslint-config-stylelint": "7.0.0",
    "npm-run-all": "3.0.0",
    "npmpub": "3.0.1",
    "remark-cli": "2.0.0",
    "remark-preset-lint-consistent": "1.0.0",
    "remark-preset-lint-recommended": "1.0.0",
    "stylelint": "8.1.1"
  },
  "dependencies": {
    "stylelint-config-standard": "17.0.0",
    "stylelint-order": "0.7.0"
  },
  "scripts": {
    "ava": "ava --verbose \"__tests__/**/*.js\"",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "release": "npmpub",
    "test": "npm run ava"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  }
}
