{
  "name": "@runroom/stylelint-config-runroom",
  "version": "1.1.9",
  "description": "Runroom config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "runroom"
  ],
  "author": {
    "name": "Runroom",
    "url": "https://runroom.es"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Runroom/stylelint-config-runroom.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "stylelint": "9.4.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.8.1",
    "stylelint-scss": "3.2.0"
  },
  "devDependencies": {
    "@runroom/eslint-config-runroom": "^0.0.2",
    "ava": "0.25.0",
    "babel-preset-env": "1.7.0",
    "npm-check-updates": "2.14.2",
    "npm-run-all": "4.1.3",
    "npmpub": "4.1.0",
    "remark-cli": "5.0.0",
    "remark-preset-lint-consistent": "2.0.2",
    "remark-preset-lint-recommended": "3.0.2"
  },
  "scripts": {
    "updeps": "ncu",
    "ava": "ava --verbose \"__tests__/**/*.js\"",
    "test": "npm run ava"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  }
}
